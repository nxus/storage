{
  "name": "@nxus/storage",
  "version": "2.3.0",
  "description": "Storage framework for Nxus applications",
  "main": "lib/",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel/register -R spec",
    "compile": "babel --optional runtime -d lib/ src/",
    "docs": "export NAME=`npm view . name`; export VERSION=`npm view . version`; ./node_modules/.bin/documentation ./src/index.js --github -f html -o ./docs --name $NAME --project-version $VERSION ",
    "prepublish": "npm run compile && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nxus/storage.git"
  },
  "keywords": [
    "nxus",
    "storage",
    "orm",
    "waterline"
  ],
  "author": "mjreich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nxus/storage/issues"
  },
  "homepage": "https://github.com/nxus/storage",
  "dependencies": {
    "babel-runtime": "^5.7.0",
    "bluebird": "^3.0.6",
    "underscore": "^1.8.3",
    "waterline": "^0.10.28"
  },
  "peerDependencies": {
    "@nxus/core": "^2.3.0"
  },
  "devDependencies": {
    "@nxus/core": "^2.3.0",
    "babel": "^5.6.23",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "documentation": "^3.0.4",
    "mocha": "^2.2.5",
    "sails-memory": "^0.10.5",
    "sinon": "^1.17.2"
  }
}
